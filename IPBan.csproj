<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <AssemblyVersion>1.3.0.0</AssemblyVersion>
    <FileVersion>1.3.0.0</FileVersion>
    <Company>Digital Ruby, LLC</Company>
    <Authors>jjxtra</Authors>
    <Copyright>(c) 2010 Digital Ruby, LLC</Copyright>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ApplicationIcon />
    <OutputType>Exe</OutputType>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <StartupObject>IPBan.IPBanMain</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="IPBanCore\**" />
    <Compile Remove="packages\**" />
    <Compile Remove="Properties\PublishProfiles\**" />
    <EmbeddedResource Remove="IPBanCore\**" />
    <EmbeddedResource Remove="packages\**" />
    <EmbeddedResource Remove="Properties\PublishProfiles\**" />
    <None Remove="IPBanCore\**" />
    <None Remove="packages\**" />
    <None Remove="Properties\PublishProfiles\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="2.0.0" />
    <PackageReference Include="NLog" Version="4.5.6" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Interop.NetFwTypeLib">
      <HintPath>..\Interop.NetFwTypeLib.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
