<?xml version="1.0"?>
<configuration>
  <appSettings>
    
    <!-- Number of failed audits in the event viewer before banning the ip address -->
    <add key="FailedLoginAttemptsBeforeBan" value="5" />
    
    <!-- The duration of time to ban a failed ip address -->
    <add key="BanTime" value="01:00:00:00" />
    
    <!-- The file to store banned ip addresses in -->
    <add key="BanFile" value="banlog.txt" />
    
    <!-- The time between cycles to check for banned ip addresses that should be unbanned -->
    <add key="CycleTime" value="00:00:01:00" />

    <!-- Prefix for windows firewall rule to block the ip address (i.e. BlockIPAddress195.55.12.33, ip is appended automatically)-->
    <add key="RulePrefix" value="BlockIPAddress" />
    
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
</configuration>
